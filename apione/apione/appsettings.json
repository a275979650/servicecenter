{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "service"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://127.0.0.1:5000/eureka/",
      "validateCertificates": false
    },
    "instance": {
      "port": 8010,
      "preferIpAddress": true,
      "instanceId": "service-container:8010"
    }
  },
  "Services": {
    "ClientService": {
      "Url": "http://client/api/values/"
    }
  }
}
